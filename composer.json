{
  "name": "antwerpes/laravel-remote-view",
  "type": "library",
  "description": "A Laravel package to fetch blade templates from a remote URL.",
  "keywords": [],
  "homepage": "http://www.antwerpes.de",
  "license": "proprietary",
  "authors": [
    {
      "name": "Thorsten Daners",
      "email": "thorsten.daners@antwerpes.de"
    }
  ],
  "require": {
    "php": "^7.2",
    "laravel/framework": "~5.5.0|~5.6.0|~5.7.0",
    "guzzlehttp/guzzle": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Antwerpes\\RemoteView\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Antwerpes\\RemoteView\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "laravel": {
      "providers": [
        "Antwerpes\\RemoteView\\RemoteViewServiceProvider"
      ]
    }
  }
}
