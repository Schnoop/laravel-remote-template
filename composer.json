{
  "name": "schnoop/laravel-remote-template",
  "type": "library",
  "description": "A Laravel package to fetch templates from a remote URL.",
  "keywords": [],
  "homepage": "https://github.com/schnoop/laravel-remote-template",
  "license": "MIT",
  "authors": [
    {
      "name": "Thorsten Daners",
      "email": "thorsten@daners.info"
    }
  ],
  "require": {
    "php": "^7.1",
    "laravel/framework": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
    "guzzlehttp/guzzle": "^6.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.2",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "Schnoop\\RemoteTemplate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Schnoop\\RemoteTemplate\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "laravel": {
      "providers": [
        "Schnoop\\RemoteTemplate\\RemoteTemplateServiceProvider"
      ]
    }
  }
}
