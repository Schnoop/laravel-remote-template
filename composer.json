{
  "name": "schnoop/laravel-remote-template",
  "type": "library",
  "description": "A Laravel package to fetch templates from a remote URL.",
  "keywords": [
    "laravel",
    "blade",
    "template",
    "remote"
  ],
  "homepage": "https://github.com/schnoop/laravel-remote-template",
  "license": "MIT",
  "authors": [
    {
      "name": "Thorsten Daners",
      "email": "thorsten@daners.info"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/contracts": "^9.0",
    "laravel/framework": "^9.2",
    "guzzlehttp/guzzle": "^7.2"
  },
  "require-dev": {
    "chiiya/laravel-code-style": "^1.7",
    "mockery/mockery": "^1.5",
    "orchestra/testbench": "^7.1",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Schnoop\\RemoteTemplate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Schnoop\\RemoteTemplate\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "phpro/grumphp": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "scripts": {
    "prepare": [
      "@composer run post-autoload-dump"
    ],
    "test": [
      "@composer run prepare",
      "@php vendor/bin/phpunit -c ./ --color"
    ],
    "lint": [
      "@php vendor/bin/ecs check --fix",
      "@php vendor/bin/php-cs-fixer fix",
      "@php vendor/bin/rector process",
      "@php vendor/bin/tlint lint"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "9.0-dev"
    },
    "laravel": {
      "providers": [
        "Schnoop\\RemoteTemplate\\RemoteTemplateServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
